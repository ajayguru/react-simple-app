    "test:jenkins": "cross-env NODE_PATH=src/ mocha --reporter mocha-junit-reporter",
    "lint": "eslint . --ext .js,.jsx",
    "lint:jenkins": "eslint . --ext .js,.jsx -f checkstyle -o eslint.xml",
    "lint-fix": "eslint . --ext .js,.jsx --fix",
    "lint-fix-test": "npm run lint-fix && npm run test"
  },
  "devDependencies": {
    "airbnb-js-shims": "1.4.1",
