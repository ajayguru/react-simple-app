  "scripts": {
    "build": "cross-env NODE_PATH=src/ node scripts/build.js",
    "coverage": "rimraf ./coverage && cross-env NODE_PATH=src/ NODE_ENV=test BABEL_DISABLE_CACHE=1 nyc mocha",
    "start": "cross-env PORT=3000 NODE_PATH=src/ node scripts/start.js",
    "test": "eslint . --ext .js,.jsx && cross-env NODE_ENV=test NODE_PATH=src/ mocha --watch",
